<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>VehicleController</servlet-name>
		<display-name>VehicleController</display-name>
		<description>This will handle all the vehicle requests</description>
		<servlet-class>com.udara.cabservice.controller.VehicleController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DriverController</servlet-name>
		<display-name>DriverController</display-name>
		<description></description>
		<servlet-class>com.udara.cabservice.controller.DriverController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<display-name>LoginController</display-name>
		<description></description>
		<servlet-class>com.udara.cabservice.controller.LoginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterController</servlet-name>
		<display-name>RegisterController</display-name>
		<description></description>
		<servlet-class>com.udara.cabservice.controller.RegisterController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterControl</servlet-name>
		<display-name>RegisterControl</display-name>
		<description></description>
		<servlet-class>com.udara.cabservice.controller.RegisterControl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VehicleController</servlet-name>
		<url-pattern>/Vehicle</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DriverController</servlet-name>
		<url-pattern>/Driver</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterController</servlet-name>
		<url-pattern>/RegisterController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterControl</servlet-name>
		<url-pattern>/RegisterControl</url-pattern>
	</servlet-mapping>
	</web-app>
	
